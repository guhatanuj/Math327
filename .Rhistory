install.packages('knitr', dependencies = TRUE)
clear
cls
clear
clean
install.packages("rmarkdown")
knitr::opts_chunk$set(echo = TRUE)
# Activate the Stat2Data package (must be installed first - one-time step)
library (Stat2Data)
# load the Day1Survey data set
data (Day1Survey)
head (Day1Survey)
# Three different ways to do the same thing
boxplot (Height ~ Group, data=Day1Survey)
# Three different ways to do the same thing
boxplot (Height ~ Sex, data=Day1Survey)
# Need to install the mosaic package (one-time step).
library (mosaic)
favstats(Height ~ Sex, data=Day1Survey)
t.test (Height ~ Sex, data=Day1Survey, var.equal=T)
library (mosaic)
favstats(Height ~ Sex, data=Day1Survey, echo = FALSE)
library (mosaic, echo=FALSE)
invisible(library (mosaic))
favstats(Height ~ Sex, data=Day1Survey, echo = FALSE)
library (mosaic, message=FALSE)
library (mosaic)
favstats(Height ~ Sex, data=Day1Survey, echo = FALSE)
library (mosaic)
favstats(Height ~ Sex, data=Day1Survey)
library (Stat2Data)
